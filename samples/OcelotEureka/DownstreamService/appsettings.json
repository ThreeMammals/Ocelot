{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": { "Default": "Warning" }
  },
  "spring": {
    "application": { "name": "ncore-rat" },
    "cloud": {
      "config": {
        "uri": "http://localhost:5001",
        "validate_certificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": false,
      "validateCertificates": false
    },
    "instance": { "port": 5001 }
  }
}

